<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <record id="action_report_helpdesk_ticket" model="ir.actions.report">
        <field name="name">Helpdesk Ticket</field>
        <field name="model">helpdesk.ticket</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">helpdesk_javierparra.report_helpdesk_ticket</field>
        <field name="report_file">helpdesk_javierparra.report_helpdesk_ticket</field>
        <field name="print_report_name">'Ticket - %s' % (object.name)</field>
        <field name="binding_model_id" ref="helpdesk_javierparra.model_helpdesk_ticket"/>
        <field name="binding_type">report</field>
    </record>

    <record id="action_report_helpdesk_ticket_list" model="ir.actions.report">
        <field name="name">Helpdesk Ticket List</field>
        <field name="model">helpdesk.ticket</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">helpdesk_javierparra.report_helpdesk_ticket_list</field>
        <field name="report_file">helpdesk_javierparra.report_helpdesk_ticket_list</field>
        <field name="print_report_name">'Ticket List'</field>
        <field name="binding_model_id" ref="helpdesk_javierparra.model_helpdesk_ticket"/>
        <field name="binding_type">report</field>
    </record>



    <record id="paperformat_user_card" model="report.paperformat">
        <field name="name">Custom Paperformat for the Event Full Page Ticket report</field>
        <field name="default" eval="False"/>
        <field name="disable_shrinking" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">120</field>
        <field name="page_width">240</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">8</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="dpi">96</field>
    </record>


    <record id="action_report_paperformat_user_card" model="ir.actions.report">
        <field name="name">User Card</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">helpdesk_javierparra.user_cart_report</field>
        <field name="report_file">helpdesk_javierparra.user_cart_report</field>
        <field name="print_report_name">'Ticket - %s' % (object.name)</field>
        <field name="binding_model_id" ref="helpdesk_javierparra.model_helpdesk_ticket"/>
        <field name="binding_type">report</field>
        <field name="paperformat_id" ref="helpdesk_javierparra.paperformat_user_card"/>
    </record>

</odoo>